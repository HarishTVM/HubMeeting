<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">
  <head>
    <!-- Site Title-->
    <title><%= translation.HUB_SPACE_TOOLBAR_DASHBOARD %></title>
 <!-- Site logo images-->
 <link href="/images/logo.png" rel="icon" type="image/x-icon"/>
 <!--Stylesheets file links -->
 <% include _layouts/common-header %>
 <link rel="stylesheet" href="/styles/spaces/createspace.css">
 <link rel="stylesheet" href="styles/meeting/newMeeting.css">
 <link rel="stylesheet" href="/styles/inbuilt/toastr.min.css">

  </head>
  <body>
    <!-- include file for navigation -->
    <% include _layouts/header-with-navigation %>
    <div class="page" >
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <div class="container-fluid breadcrumb-container">
          <ol class="breadcrumb">
              <h5 class="meeting-head-bread"><%= translation.HUB_CREATE_SPACE_TITLE %></h5>
              <li><a href="/dashboard" class="breadcrumb-links"><%= translation.HUB_SPACES_TOOLBAR_DASHBOARD %></a></li>
              <li><a href="/spacelist" class="breadcrumb-links"><%= translation.HUB_SPACES_TOOLBAR_SPACES %></a></li>
              <li><%= translation.HUB_CREATE_SPACE_TOOLBAR %></li>
          </ol>
      </div>
      <section class="section-lg bg-white text-center main-section">
        <div class="shell">
          <h4 id="createSpace"><%= translation.HUB_CREATE_SPACE_TOOLBAR %></h4>
          <h4 id="updateSpace">Update Cospace</h4>
          <div class="range range-md-center">
            <div class="cell-md-10 cell-lg-8">

            <form class="rd-mailform rd-mailform_style-1 text-center validate-form">
              <div id="Name"class="form-wrap form-wrap_icon linear-icon-database-add">
                <input class="form-input input-box-shadow validate-text" id="contact-sName" type="text" name="name">
                <label class="form-label" for="contact-sName"><%= translation.HUB_CREATE_SPACE_NAME %></label>
                <span class="hide error-text"><%= translation.CREATE_SPACE_ERROR_TEXT_SPACE_NAME %></span>
              </div>
              <div id="URI"class="form-wrap form-wrap_icon linear-icon-link2">
                  <input class="form-input input-box-shadow validate-text" id="contact-create_space_URI" type="text" name="create_space_URI">
                  <label class="form-label" for="contact-create_space_URI"><%= translation.HUB_CREATE_SPACE_ENTER_URI %></label>
                  <span class="hide error-text"><%= translation.CREATE_SPACE_ERROR_TEXT_ENTER_URI %></span>
                </div>
              <!-- <div class="form-wrap form-wrap_icon linear-icon-users-plus">
              <input class="form-input input-box-shadow validate-text" id="contact-sMember" type="email" name="email">
              <label class="form-label" for="contact-sMember"><%= translation.HUB_CREATE_SPACE_TYPE_MEMBER_NAME %></label>
              <span class="hide error-text"><%= translation.CREATE_SPACE_ERROR_TEXT_TYPE_MEMBER_NAME %></span>
            </div> -->
            <div class="form-wrap form-wrap_icon linear-icon-border-all margin-css custom-cospace input-box-shadow">
              <select class="form-control on-hover-color " id="sel1">
                  <option value="" selected disabled><%= translation.HUB_CREATE_SPACE_DEFAULT_LAYOUT %></option>
                  <option><%= translation.HUB_CREATE_SPACE_ALLEQUAL %></option>
                  <option><%= translation.HUB_CREATE_SPACE_SPEAKERONLY %></option>      
                  <option><%= translation.HUB_CREATE_SPACE_TELEPRESENCE %></option>
                  <option><%= translation.HUB_CREATE_SPACE_STACKED %></option> 
                  <option><%= translation.HUB_CREATE_SPACE_ALLEQUALQUARTERS %></option>   
                  <option><%= translation.HUB_CREATE_SPACE_ALLEQUALNINTHS %></option>     
              </select>
            </div>
            <!-- <div class="form-wrap form-wrap_icon linear-icon-telephone margin-css custom-cospace input-box-shadow">
              <select id="sel2">
                  <option value="" selected disabled>Select the Require CallId</option>
                  <option>True</option>
                  <option>False </option>
              </select>
            </div> -->
            <div class="form-wrap form-wrap_icon linear-icon-key-hole">
              <input class="form-input input-box-shadow validate-text alidate-text" id="contact-sPasscode" type="text" name="passcode">
              <label class="form-label" for="contact-sPasscode"><%= translation.HUB_CREATE_SPACE_PASSCODE %></label>
              <span class="hide error-text"><%= translation.CREATE_SPACE_ERROR_TEXT_PASS_CODE %></span>              
            </div>
            <!-- <div class="form-wrap form-wrap_icon linear-icon-home6 ">
              <input class="form-input input-box-shadow validate-text" id="contact-sAddress" type="text" name="email">
              <label class="form-label" for="contact-sAddress"><%= translation.HUB_CREATE_SPACE_ADDRESS %></label>
              <span class="hide error-text"><%= translation.CREATE_SPACE_ERROR_TEXT_SPACE_ADDRES %></span>
              
              </div> -->
            
              <button id="btndiscard" class="button btn_discard " type="submit"><%= translation.HUB_CREATE_SPACE_DISCARD %> </button>
              &nbsp;&nbsp;
              &nbsp;&nbsp;
             <button id="btndone" class="button button-primary" type="submit"><%= translation.HUB_CREATE_SPACE_DONE %></button>
            </form>
          </div>
        </div>
       </div>
      </section>
    </div>

      <!-- include file for Script -->
      <% include _layouts/common-scripts %>
      <script src="/scripts/inbuilt/toastr.min.js"></script>
      <script src="/scripts/common/validation.js"></script>
      <script src="/scripts/spaces/createspace.js"></script>
      <script src="/scripts/spaces/updateCospace.js"></script>
    <script>
        $(document).ready(function(){
            $("#spacesList").addClass("active");
        });
    </script>

    
<%include _layouts/footer %>  
  </body>
</html>
