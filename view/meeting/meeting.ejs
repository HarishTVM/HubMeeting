<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">
  <head>
    <!-- Site Title-->
    <title><%= translation.HUB_MEETING_PAGE_TITLE %></title>
    <!-- Site logo images-->
    <link href="/images/logo.png" rel="icon" type="image/x-icon"/>
    <link rel="stylesheet" href="/styles/meeting/meeting.css">
    <!--Stylesheets file links -->
    <% include _layouts/common-header %>
  </head>
  <body class=" bg-white">
    <!-- include file for navigation -->
    <% include _layouts/header-with-navigation %>
    <div class="page">
      <!-- BEGIN page loader -->
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <!-- End page loader -->
      <!-- BEGIN Meeting toolbar-->
      <div class="container-fluid breadcrumb-container">
        <ol class="breadcrumb">
          <h5 class="meeting-head-bread"><%= translation.HUB_MEETING_SUBHEAD_MEETING %></h5>
          <li><a href="/dashboard" class="breadcrumb-links"><%= translation.HUB_MEETING_BREADCRUMB_DASHBOARD %></a></li>
          <li><%= translation.HUB_MEETING_BREADCRUMB_MEETING %></li>
        </ol>
      </div>
      <!-- END Meeting toolbar-->
      <section>
        <!-- BEGIN Meeting pagination head -->
        <div class="shell">
          <!-- BEGIN Meeting search head -->
          <div class="range range-5 h-m-top-none">
            <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12">
              <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-8">
                <div class="form-wrap form-wrap_icon linear-icon-magnifier head-search-w-h search-margin-top">
                  <input class="form-input search-input input-box-shadow " type="text" name="search" placeholder="Enter the Meeting">
                </div>
              </div>
              <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-4">
                <div class="text-margin-top">

                </div>
              </div>
            </div>
          </div>
          <!-- END Meeting search head -->
          <div class="row range range-70">
            <!-- include file for card-->
            <% include _layouts/cards/card-meeting %>
          </div>
          <!-- ceateMeeting -->
          <a href="/newMeeting" data-toggle="tooltip" data-placement="bottom" title=""
            class="button button-primary  linear-icon-plus newmeeting-btn-circle"
            data-original-title="<%= translation.HUB_MEETING_CREATE_MEETING_BTN %>"></a>
        </div>
      </div>
    </section>
    <!-- BEGIN script file links -->
    <% include _layouts/common-scripts %>


    <script>
    $(document).ready(function(){
      $("#meetingList").addClass("active");
    });
    </script>
    <!-- End Meeting script -->
    <!-- Pagination next footer -->
    <%include _layouts/footer %>
  </body>
</html>
