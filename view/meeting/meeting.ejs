<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">

<head>
  <!-- Site Title-->
  <title>
    <%= translation.HUB_MEETING_PAGE_TITLE %>
  </title>
  <!-- Site logo images-->
  <link href="/images/logo.png" rel="icon" type="image/x-icon" />
  <link rel="stylesheet" href="/styles/meeting/meeting.css">
  <link rel="stylesheet" href="/styles/common/sweetalert.css">
  <!--Stylesheets file links -->
  <% include _layouts/common-header %>
</head>

<body>
  <!-- include file for navigation -->
  <% include _layouts/header-with-navigation %>
    <div class="page">
      <!-- BEGIN page loader -->
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <!-- End page loader -->
      <!-- BEGIN Meeting toolbar-->
      <div class="container-fluid breadcrumb-container">
        <ol class="breadcrumb">
          <h5 class="meeting-head-bread t-c-default-toolbar">
            <%= translation.HUB_MEETING_SUBHEAD_MEETING %>
          </h5>
          <li>
            <a href="/dashboard" class="breadcrumb-links">
              <%= translation.HUB_MEETING_BREADCRUMB_DASHBOARD %>
            </a>
          </li>
          <li>
            <%= translation.HUB_MEETING_BREADCRUMB_MEETING %>
          </li>
        </ol>
      </div>
      <!-- END Meeting toolbar-->
      <section>
        <div class="shell">
          <!-- BEGIN users search head -->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center">
            <!-- Search input field -->
            <div class="form-wrap form-wrap_icon linear-icon-magnifier head-search-w-h">
              <input id="filter-meeting" class="form-input cursor-auto search-input input-box-shadow" type="text" name="search" placeholder="Search Meeting">
            </div>
          </div>
          <!-- handlebars -->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center">
            <div class="all-card-margin-top" id="meeting-card">
            </div>
          </div>
          <% include _layouts/cards/meeting-card %>
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center">
            <div class="modal"></div>
          </div>

          <!-- include file for card-->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center">
              <% include _layouts/cards/model %>
          </div>

          

        </div>
        <!-- page-loaders-users -->
        <div class="us-card-loading" id="page-loaders-users">
          <div class="cssload-container">
            <div class="cssload-speeding-wheel"></div>
          </div>
        </div>

        <!-- createMeeting -->
        <a href="/newMeeting" data-toggle="tooltip" data-placement="bottom" title="" class="button button-primary  linear-icon-plus newmeeting-btn-circle"
          data-original-title="<%= translation.SCHEDULE_MEETING_BREADCRUMB_NEW_MEETING %>"></a>
        <!-- End Users Pages-->
      </section>
    </div>
    <!-- BEGIN script file links -->
    <% include _layouts/common-scripts %>
      <script type="text/javascript" src="/scripts/meeting/meeting.js"></script>
      <script src="/scripts/meeting/updatemeeting.js"></script>
      <script src="/scripts/spaces/updatespace.js"></script>
      <script src="/scripts/jquery.twbsPagination.min.js"></script>
      <script src="/scripts/inbuilt/toastr.min.js"></script>
      <script src="/scripts/common/sweetalert.min.js"></script>
      <script>
        $(document).ready(function () {
          $("#meetingList").addClass("active");
        });
      </script>
      <!-- End Meeting script -->
      <%include _layouts/footer %>

</body>

</html>