<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">

<head>
  <!-- Site Title-->
  <title>
    <%= translation.HUB_MEETING_PAGE_TITLE %>
  </title>
  <!-- Site logo images-->
  <link href="/images/logo.png" rel="icon" type="image/x-icon" />
  <link rel="stylesheet" href="/styles/meeting/meeting.css">
  <link rel="stylesheet" href="/styles/meeting/meeting.css">
  <link rel="stylesheet" href="/node_modules/sweetalert2/dist/sweetalert2.min.css">
  <!--Stylesheets file links -->
  <% include _layouts/common-header %>
</head>

<body>
  <!-- include file for navigation -->
  <% include _layouts/header-with-navigation %>
    <div class="page">
      <!-- BEGIN page loader -->
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <!-- End page loader -->
      <!-- BEGIN Meeting toolbar-->
      <div class="container-fluid breadcrumb-container">
        <ol class="breadcrumb">
          <h5 class="meeting-head-bread t-c-default-toolbar">
            <%= translation.HUB_MEETING_SUBHEAD_MEETING %>
          </h5>
          <li>
            <a href="/dashboard" class="breadcrumb-links">
              <%= translation.HUB_MEETING_BREADCRUMB_DASHBOARD %>
            </a>
          </li>
          <li>
            <%= translation.HUB_MEETING_BREADCRUMB_MEETING %>
          </li>
        </ol>
      </div>
      <!-- END Meeting toolbar-->
      <section>
        <div class="shell">
          <!-- BEGIN meeting search head -->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center">
            <!-- Search input field -->
            <div class="form-wrap form-wrap_icon linear-icon-magnifier head-search-w-h">
              <input id="searhMeeting" class="form-input cursor-auto search-input input-box-shadow" type="text" name="search" placeholder="Search Meeting">
            </div>
          </div>

          <!--Pagination plugin-->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 text-center padding-left-right-none">
            <ul class="sync-pagination pagination-sm"></ul>
          </div>

          <!-- handlebars -->
          <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none text-center meeting-card-div">
            <div id="meetingCardHandlebars">

            </div>
          </div>

          <!-- include file for card-->
          <% include _layouts/cards/meeting-card %>
            <% include _layouts/modals/meeting-modal %>
        </div>
        <!-- page-loaders-meeting -->
        <div class="us-card-loading" id="page-loaders-meeting">
          <div class="cssload-container">
            <div class="cssload-speeding-wheel"></div>
          </div>
        </div>
        <!-- Begin delete Modal -->
        <div id="delModal" class="modal fade" role="dialog">
          <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
              <div class="modal-body">
                <h6>Are you sure you want to delete ?</h6>
              </div>
              <div class="modal-footer">
                <button id="yesMeetingDelBtn" type="button" class="button button-primary button-sm">Yes</button>
                <button id="noMeetingDelBtn" type="button" class="button button-primary button-sm">No</button>
              </div>
            </div>
          </div>
        </div>
        <!-- End delete Modal -->
        <!-- createMeeting -->
        <a href="/newmeeting" data-toggle="tooltip" data-placement="bottom" title="" class="button button-primary  linear-icon-plus newmeeting-btn-circle"
          data-original-title="<%= translation.SCHEDULE_MEETING_BREADCRUMB_NEW_MEETING %>"></a>
        <!-- End Users Pages-->
      </section>
      <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 text-center padding-left-right-none">
        <ul class="sync-pagination pagination-sm "></ul>
        <!--Pagination plugin-->
      </div>
    </div>
    <!-- BEGIN script file links -->
    <% include _layouts/common-scripts %>
      <script src="/scripts/meeting/meeting.js"></script>
      <script src="/scripts/meeting/updatemeeting.js"></script>
      <script src="/scripts/spaces/updatespace.js"></script>
      <script src="/scripts/jquery.twbsPagination.min.js"></script>
      <script src="/scripts/inbuilt/toastr.min.js"></script>
      <script src="/node_modules/sweetalert2/dist/sweetalert2.all.min.js"></script>
      <script>
        $(document).ready(function () {
          $("#meetingList").addClass("active");
        });
      </script>
      <!-- End Meeting script -->
      <%include _layouts/footer %>

</body>

</html>