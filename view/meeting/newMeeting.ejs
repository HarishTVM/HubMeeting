<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">
  <head>
    <!-- Site Title-->
    <title><%= translation.SCHEDULE_MEETING_PAGE_TITLE %></title>
     <!-- Site logo images-->
     <link href="/images/logo.png" rel="icon" type="image/x-icon"/>
     <!--Stylesheets file links -->
     <% include _layouts/common-header %>
    <link rel="stylesheet" href="/styles/newmeeting.css">
  </head>
  <body>
   <!-- include file for navigation -->
   <% include _layouts/header-with-navigation %>
    <div class="page">
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <div class="container-fluid breadcrumb-container">
          <ol class="breadcrumb">
              <h5 class="newmeeting-head-bread"><%= translation.SCHEDULE_MEETING_HEADER %></h5>
              <li><a href="/dashboard" class="breadcrumb-links"><%= translation.SCHEDULE_MEETING_BREADCRUMB_DASHBOARD %></a></li>
              <li><a href="/meeting" class="breadcrumb-links"><%= translation.SCHEDULE_MEETING_BREADCRUMB_MEETING %></a></li>
              <li><%= translation.SCHEDULE_MEETING_BREADCRUMB_NEW_MEETING %></li>
          </ol>   
      </div>
      <section class="section-lg bg-white text-center main-section">
        <div class="shell">
		    <!-- <h4><%= translation.SCHEDULE_MEETING_HEADER %></h4> -->
          <div class="range range-md-center">
            <div class="cell-md-10 cell-lg-8">
              <!-- RD Mailform-->
              <form class="rd-mailform rd-mailform_style-1 text-center" data-form-output="form-output-global" data-form-type="contact" method="post" action="bat/rd-mailform.php">
                <div class="form-wrap form-wrap_icon linear-icon-man margin-css title-wrap">
                  <input class="form-input" id="title" type="text" name="title" data-constraints="@Required">
                  <label class="form-label" for="title"><%= translation.SCHEDULE_MEETING_TITLE %></label>
                </div>
                <div class="div1 margin-css">
                  <div class="form-wrap form-wrap_icon linear-icon-key-hole email-wrap">
                    <input class="form-input" id="contact-email" type="email" name="email" data-constraints="@Required">
                    <label class="form-label" for="contact-email"><%= translation.SCHEDULE_MEETING_EMAIL %></label>
                  </div>&nbsp;&nbsp;
                  <div class="form-wrap form-wrap_icon linear-icon-telephone telephone-wrap">
                    <input class="form-input" id="contact-phone" type="text" name="phone" data-constraints="@Required">
                    <label class="form-label" for="contact-phone"><%= translation.SCHEDULE_MEETING_PHONE %></label>
                  </div>
                </div>
                <div class="row form-wrap datetime-picker-container margin-css">
                  <div class="form-wrap_icon form-wrap_icon__label-outside linear-icon-calendar-31 from-to-date-container">
                      <!-- <label class="form-label-outside" for="event-date">Date</label> -->
                      <input class="form-input" id="fromdate" type="text" name="fromdate" placeholder="Start-date" data-time-picker="date" data-dtp="dtp_HoYzJ">
                  </div>&nbsp;&nbsp;	
                  <div class="form-wrap form-wrap_icon form-wrap_icon__label-outside linear-icon-timer2 from-to-date-container">
                      <!-- <label class="form-label-outside" for="event-date">Date</label> -->
                      <input class="form-input" id="fromdate" type="text" name="fromtime" placeholder="Start-time" data-time-picker="time" data-dtp="dtp_HoYzJ">
                  </div>
                </div>
                <div class="row form-wrap datetime-picker-container margin-css">
                  <div class="form-wrap_icon form-wrap_icon__label-outside linear-icon-calendar-31 from-to-date-container">
                    <!-- <label class="form-label-outside" for="event-date">Date</label> -->
                    <input class="form-input" id="todate" type="text" name="todate" placeholder="End-date" data-time-picker="date" data-dtp="dtp_HoYzJ">
                  </div>&nbsp;&nbsp;	
                  <div class="form-wrap form-wrap_icon form-wrap_icon__label-outside linear-icon-timer2 from-to-date-container">
                    <!-- <label class="form-label-outside" for="event-date">Date</label> -->
                    <input class="form-input" id="todate" type="text" name="totime" placeholder="End-time" data-time-picker="time" data-dtp="dtp_HoYzJ">
                  </div>
                </div>  
                <div class="div2 margin-css">
                  <div class="form-wrap form-wrap_icon linear-icon-timer timezone-wrap">
                    <input class="form-input" id="timezone" type="text" name="timezone" data-constraints="@Required">
                    <label class="form-label" for="timezone"><%= translation.SCHEDULE_MEETING_TIMEZONE %></label>
                  </div>&nbsp;&nbsp;
                  <div class="form-wrap form-wrap_icon linear-icon-group-work cospace-wrap">
                    <input class="form-input" id="cospace" type="text" name="cospace" data-constraints="@Required">
                    <label class="form-label" for="cospace"><%= translation.SCHEDULE_MEETING_COSPACE %></label>
                  </div>
                </div>  
                <div class="form-wrap form-wrap_icon linear-icon-group-work margin-css">
                  <select class="form-control" id="sel2">
                      <option value="" selected disabled><%= translation.SCHEDULE_MEETING_COSPACE_TYPE %></option>
                      <option><%= translation.SCHEDULE_MEETING_DEFAULT_TEMPLATE %></option>
                      <option><%= translation.SCHEDULE_MEETING_HIDDEN_ROOM %></option>      
                      <option><%= translation.SCHEDULE_MEETING_MANAGED_MEETING %></option>
                      <option><%= translation.SCHEDULE_MEETING_ALL_HANDS %></option>   
                  </select>
                </div>
                <h5><%= translation.SCHEDULE_MEETING_MEMBER %></h5>
                <div class="form-wrap form-wrap_icon linear-icon-users2">
                  <input class="form-input" id="participants" type="text" name="participants" data-constraints="@Required">
                  <label class="form-label" for="participants"><%= translation.SCHEDULE_MEETING_PARTICIPANTS %></label>
                </div>
                <div class="form-wrap form-wrap_icon linear-icon-location">
                  <input class="form-input" id="pin" type="text" name="pin" data-constraints="@Required">
                  <label class="form-label" for="pin"><%= translation.SCHEDULE_MEETING_PIN %></label>
                </div> 
              </form>
              <br>
              <div class="header-div">
                <a href="/meeting" class="button cancel-btn text-center"><%= translation.SCHEDULE_MEETING_CANCEL %></a>
                <a href="/meeting" class="button button-primary done-btn"><%= translation.SCHEDULE_MEETING_DONE %></a>
              </div>  
            </div>
          </div>
        </div>
      </section>
      </div>
         <!-- BEGIN script file links -->
    <% include _layouts/common-scripts %>
    <script>
        $(document).ready(function(){
            $("#meetingList").addClass("active");
        });
    </script>   <!-- Pagination next footer -->
    <%include _layouts/footer %>
  </body>
</html>