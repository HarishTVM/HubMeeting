            <script src="/scripts/script.js"></script>
            <script>
                $('[data-toggle="tooltip"]').tooltip();
                $(window).load(function () {
                    var hoverEle = null, isHovered = false;
                    $('.child2').hide();
                    $('.parentCell').css({'color': 'black', 'fontWeight': 'normal'});
                    $('.closeBtn').hide();

                    $('.infoBtn').click(function() {
                        if(!isHovered){
                            $(this).siblings('.child2').show();
                            $(this).parent().animate({
                                width: 300,
                                height: 400
                            }, 'fast');
                            $(this).parent().animate().css({'box-shadow': '3px 3px 5px lightgray'});
                            $(this).parent().css({'opacity': 0.8, 'color': 'black', 'fontWeight': '900', 'position': 'absolute'});
                            $(this).parent().css({zIndex: 100});
                            $(this).siblings('.closeBtn').show();
                            $(this).hide();
                            hoverEle = $(this).parent('.parentCell');
                            isHovered = true;
                        }
                    });

                    $('.closeBtn').click(function () {
                        if (hoverEle != null) {
                            $(hoverEle).find('.child2').hide();
                            $(hoverEle).css({'opacity': 1, 'color': 'black', 'fontWeight': 'normal', 'position': 'relative'})
                            $(hoverEle).animate().css({'box-shadow': 'none'});
                            $(hoverEle).animate({
                                width: 300,
                                height: 200
                            }, 'fast');
                            $(hoverEle).css({zIndex: 1});
                            hoverEle = null;
                            isHovered = false;
                            $('.parentCell').find('.closeBtn').hide();
                            $('.parentCell').find('.infoBtn').show();
                        }
                    });
                });
            </script>
