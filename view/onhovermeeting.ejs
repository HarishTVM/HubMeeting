    <script src="/node_modules/jquery/dist/jquery.min.js"></script>   
    <script src="/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/scripts/core.min.js"></script>
    <script src="/scripts/script.js"></script>
    <script>
        $('[data-toggle="tooltip"]').tooltip(); 
        $(window).load(function(){
        $('.child2').hide();
        $('.parentCell').css('color', 'black');
        $('.parentCell').css('fontWeight', 'normal');
            $('.parentCell').hover(function(){
                $(this).find('.child2').show();
                $(this).animate({
                    height: 400,
                    top: -50,
                    left: -40
                }, 'fast');
                $(this).animate().css( 'box-shadow', '5px 5px 5px lightgray');
                $(this).css('opacity', '0.8');
                $(this).css('color', 'black');
                $(this).css('fontWeight', '900');
                $(this).css({
                    zIndex: 1,
                }); 
            }, function(){
                $(this).find('.closeBtn').click(function(){ 
                    $(this).find('.child2').hide();
                    $(this).animate().css( 'box-shadow', 'none');
                    $(this).css('opacity', '1');
                    $(this).css('color', 'black');
                    $(this).css('fontWeight', 'normal');
                    $(this).animate({
                        height: 200,
                        top: 0,
                        left: 0
                    }, 'fast');
                    $(this).css({
                        zIndex: 1
                    });
            });
        }); 
    });       
    </script>
