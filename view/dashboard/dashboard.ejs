      y: 100,
      r: 5
    }],
    backgroundColor: "#3a8fff"
  }]
};

            var ctx = document.getElementById("canvas").getContext("2d");
            var myChart = new Chart(ctx, {
                type: 'bubble',
                data: popData,
                options: {
                    responsive: true,
                    title:{
                        display:false,
                        text:'<%= translation.HUB_DASHBORD_BUBBLE_CHART_MEETING_DURATION %>'
                    },
                    tooltips: {
                        mode: 'point'
                    }
                }
            });

            Highcharts.chart('container', {
                chart: {
                    type: 'pie'
                },
                title: {
                    text: '<%= translation.HUB_DASHBORD_PIE_CHART_MEETINGS %>'
                },
                // subtitle: {
                //     text: 'Click the slices to view versions. Source: netmarketshare.com.'
                // },
                plotOptions: {
                    series: {
                        dataLabels: {
                            enabled: true,
                            format: '{point.name}: {point.y:.1f}%'
                        }
                    }
                },

                tooltip: {
                    headerFormat: '<span style="font-size:11px">{series.name}</span><br>',
                    pointFormat: '<span style="color:{point.color}">{point.name}</span>: <b>{point.y:.2f}%</b> of total<br/>'
                },
    series: [{
        name: '<%= translation.HUB_DASHBORD_PIE_CHART_MEETINGS_BRANDS %>',
        colorByPoint: true,
        data: [{
            name: '<%= translation.HUB_DASHBORD_PIE_CHART_MEETINGS_BRANDS_ACTIVE %>',
