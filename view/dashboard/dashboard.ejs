    <script>
        $(document).ready(function(){
            debugger;
            $("#dashboard").addClass("active");

            var popData = {
  datasets: [{
    label: ['<%= translation.HUB_DASHBORD_BUBBLE_CHART_MEETING_DURATION %>'],
    data: [{
      x: 100,
      y: 0,
      r: 10
