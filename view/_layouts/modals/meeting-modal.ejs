<!-- Modal -->
<div id="meeting-Modal" class="modal fade" role="dialog" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog modal-lg">
    <!-- Modal content-->
    <div class="modal-content modal-margin-t">
      <div class="modal-header">
        <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none ">
          <div class="col-md-6 col-sm-6 col-xs-12 col-lg-10 padding-left-right-none ">
            <h6 id="modalTitleMeeting" class="space-details h6-title-color text-overflow-title h6-title-medium us-title-color modal-t-align-l">
              <!-- Allowed RODC Password Replication Group.users -->
            </h6>
          </div>
          <div class="col-md-6 col-sm-6 col-xs-12 col-lg-2 padding-left-right-none">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
          </div>
        </div>
      </div>

      <!-- BEGIN Modal Body -->
      <div class="modal-body padding-none-all">

        <div class="content-style-form content-style-form-1">
          <!-- BEGIN Space Details -->
          <div class="col-sm-6 col-md-4 col-xs-12 col-lg-6 padding-left-right-none h-m-top-none modal-border-r">
            <div class="sl-model-bg-color sl-model-height">
              <!-- BEGIN Set Title-->
              <div class="col-sm-12 col-md-12 col-xs-12 col-lg-12  modal-toolbar-border-t-b padding-left-right-none">
                <div class="container-fluid breadcrumb-container">
                  <ol class="toolbar-padding" style="text-align: left">
                    <h5 class="text-color-default ">
                      Meeting Details
                    </h5>
                  </ol>
                </div>
              </div>
              <!-- End Set Title-->

              <!-- BEGIN Pages Loading -->
              <div class="loading-modal body-center-details">
                <div class="cssload-container">
                  <div class="cssload-speeding-wheel"></div>
                </div>
              </div>
              <!-- End Pages Loading -->
              <!-- BEGIN Set Details -->

              <div class="col-sm-12 col-md-12 col-xs-12 col-lg-12 padding-left-right-none">
                <!-- <div class="us-card-loading card-loaders-meeting">
                  <div class="cssload-container">
                    <div class="cssload-speeding-wheel"></div>
                  </div>
                </div> -->
                <div id="mainParent" class="s-height m-details-padding">
                  <div class="col-md-12 col-sm-12  col-xs-12 col-lg-12 padding-left-right-none">
                    <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none meeting-div">
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-6 padding-left-right-none meeting-div">
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-2 padding-left-right-none meeting-div">
                          <h6 class="linear-icon-calendar-31 m-details-color"></h6>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-10 padding-left-right-none meeting-div">
                          <h6 id="modalStartDate" class="cursor-default  h6-title text-overflow-title"></h6>
                        </div>

                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-6 padding-left-right-none meeting-div">
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-2 padding-left-right-none meeting-div">
                          <h6 class="linear-icon-clock3 m-details-color"></h6>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-10 padding-left-right-none ">
                          <h6 id="modalStartTimeAndEnd" class="cursor-default h6-title text-overflow-title"></h6>
                        </div>
                      </div>
                    </div>

                    <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none meeting-div card-m-top">
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-6 padding-left-right-none meeting-div">
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-2 padding-left-right-none meeting-div">
                          <h6 class="linear-icon-users2 m-details-color"></h6>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-10 padding-left-right-none meeting-div">
                          <h6 id="modalTotalMembers" class="cursor-default h6-title text-overflow-title meeting-div "></h6>
                        </div>

                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-6 padding-left-right-none meeting-div">
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-2 padding-left-right-none meeting-div">
                          <h6 class="linear-icon-border-all m-details-color"></h6>
                        </div>
                        <div class="col-md-6 col-sm-6 col-xs-6 col-lg-10 padding-left-right-none meeting-div" data-toggle=" " title="{{meeting.defaultLayout}}">
                          <h6 id="modalDefaultLayout" class="cursor-default h6-title text-overflow-title "></h6>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none meeting-div card-m-top">
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-1 padding-left-right-none meeting-div">
                        <h6 class="linear-icon-group-work m-details-color"></h6>
                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-12 col-lg-11 padding-left-right-none meeting-div">
                        <h6 id="modalMeetingType" class="cursor-default h6-title text-overflow-title"></h6>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- End Set Details -->
            </div>
          </div>
          <!-- End Space Details -->

          <!-- BEGIN Members List -->
          <div class="col-sm-6 col-md-4 col-xs-12 col-lg-6 padding-left-right-none h-m-top-none ">
            <div class="sl-model-height">
              <!-- BEGIN Set Title-->
              <div class="col-sm-12 col-md-12 col-xs-12 col-lg-12 modal-toolbar-border-t-b padding-left-right-none">
                <div class="col-sm-12 col-md-12 col-xs-12 col-lg-6">
                  <div class="container-fluid breadcrumb-container">
                    <ol class="toolbar-padding" style="text-align: left">
                      <h5 class="text-color-default ">
                        Members
                      </h5>
                    </ol>
                  </div>
                </div>
                <div class="col-sm-12 col-md-12 col-xs-12 col-lg-6">

                  <form class="padding-top-none search-m-t">
                    <div class="input-group">
                      <input id="inputSearchMeetingMembers" type="text" class="form-control sharch-width-modal" name="searchMembers" placeholder="Search Members">
                      <span id="btnSearchMembers"  class="input-group-addon input-search cursor-pointer ">
                        <i class="glyphicon linear-icon-magnifier searchMembers"></i>
                      </span>
                    </div>
                    <div class="t-right noMembersRefreshIcons cursor-pointer ">
                          <a onclick="refreshPage()" class="linear-icon-undo2"></a>
                        </div>
                  </form>
                </div>
              </div>
              <!-- End Set Title-->
              <div class="modal-padding-member sl-model-scroll-member">
                <!-- BEGIN Pages Loading -->
                <div class="loading-moda-members body-center">
                  <div class="cssload-container">
                    <div class="cssload-speeding-wheel"></div>
                  </div>
                </div>
                <!-- End Pages Loading -->

                <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none ">
                  <!-- BEGIN Set Meeting members -->
                  <div class="meeting-members-list  cursor-default" id="meetingMembersModal"></div>
                  <!-- End Set Meeting members -->
                </div>

              </div>
            </div>
          </div>
          <!-- End Members List -->
        </div>
      </div>
      <!-- End Modal Body -->
      <div class="modal-footer">
        <button id="editmodelBtn" type="button" class="btn-padding-edit button button-primary" data-dismiss="modal">Edit</button>
      </div>
    </div>
  </div>
</div>

<!-- BEGIN Set Meeting Members Handlebars-->
<script id="meetingMembers" type="text/x-handlebars-template">
      {{#each rows}}
      <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12 padding-left-right-none h-div-border sl-div-shadow-member meeting-members-list">
          <!-- BEGIN Set Meeting members -->
          <ul class="padding-left-right-none cursor-default" data-toggle=" " title="  {{memberJid}}">
              <li class="padding-left-right-none">
                  <b>
                      <h6 class="title-color text-padding-m-t padding-left-right-none title-align text-margin-top text-overflow-title">
                          {{#if memberJid}}
                          {{memberJid}}
                            {{else}}
                           <p>No Members</p>
                             {{/if}}
                        </h6>
                  </b>
              </li>
          </ul>
      </div>
      {{/each}}
    </script>
<!-- End Set Meeting Members Handlebars-->