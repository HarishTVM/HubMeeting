<!-- BEGIN Page card -->
<script id="cardId" type="text/x-handlebars-template">
{{#each coSpaces}}
<div class="col-sm-6 col-md-4 col-xs-12 col-lg-3">
  <div class="morph-button div-top thumb-corporate morph-button-modal morph-button-modal-2 morph-button-fixed  bg-white h-box-shadow h-div-border s-height"
    coSpaceId={{coSpace.attrkey.id}} onclick="myFunc(this)">
    <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 ">
      <!-- BEGIN card one user details for Spaces -->
      <ul data-toggle=" " title="{{coSpace.name}}" id="coSpaceName" coSpaceName={{coSpace.name}}>
        <li>
          <b>
            <h6 class="h6-title-color text-overflow-title h6-title-medium cursor-default">{{coSpace.name}}</h6>
          </b>
        </li>
      </ul>
      <!-- <ul id="code" code={{coSpace.passcode}}></ul>
          <ul id="uri" uri={{coSpace.uri}}></ul>
          <ul id="defLay" defLay={{coSpace.defaultLayout}}></ul> -->
      <!-- END card one user details for Spaces-->
      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title">AutoGenerated:</h6>
      </div>
      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title text-overflow-title">{{coSpace.autoGenerated}}</h6>
      </div>
    </div>
    <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12">
      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title">Space URI:</h6>
      </div>
      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title text-overflow-title">{{coSpace.uri}}</h6>
      </div>
    </div>
    <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12">

      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title">DefaultLayout:</h6>
      </div>
      <div class="col-sm-6 col-md-6 col-xs-12 col-lg-6 padding-left-right-none">
        <h6 class="h6-title text-overflow-title">{{coSpace.defaultLayout}}</h6>
      </div>

    </div>

    <!-- BEGIN card mouseover show icon for Spaces-->
    <div class="col-md-12 col-sm-12 col-md-12 col-xs-12 col-lg-12 hover-icon-bottom ">
      <div id="icon-list" class="thumb-corporate__overlay thumb-corporate__overlay-gl text-center sl-card-mardin-top">
        <ul class="list-inline-sm">
          <li>
            <p class="btn actbtn icon-info h-icon-info-sm btn-circle thumb-corporate__list h-iconls-color icon-sm" data-toggle="modal"
              data-target="#myModal" title="<%= translation.HUB_SPACES_TOOLTIP_INFO %>">
              <i class="linear-icon-reading"></i>
            </p>
          </li>
          <li>
            <p id="deleteBtn" class="btn btn-circle thumb-corporate__list h-icon-color-sm-delete btn-actived-delete icon-sm" data-toggle="tooltip"
              data-placement="top" title="<%= translation.HUB_SPACES_TOOLTIP_DELETE %>">
              <i class="linear-icon-trash2"></i>
            </p>
          </li>
        </ul>
      </div>
    </div>
    <!-- END card one mouseover show for Spaces-->
  </div>
</div>
{{/each}}
    </script>
<!-- END Page card -->