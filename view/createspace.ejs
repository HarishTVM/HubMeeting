<!DOCTYPE html>
<html class="wide wow-animation smoothscroll" lang="en">
  <head>
    <!-- Site Title-->
    <title><%= translation.HUB_SPACE_TOOLBAR_DASHBOARD %></title>
 <!-- Site logo images-->
 <link href="/images/logo.png" rel="icon" type="image/x-icon"/>
 <!--Stylesheets file links -->
 <% include _layouts/common-header %>



  </head>
  <body>
    <!-- include file for navigation -->
    <% include _layouts/header-with-navigation %>
    <div class="page" >
      <div id="page-loader">
        <div class="cssload-container">
          <div class="cssload-speeding-wheel"></div>
        </div>
      </div>
      <div class="container-fluid breadcrumb-container">
          <ol class="breadcrumb">
              <h5 class="meeting-head-bread"><%= translation.HUB_CREATE_SPACE_TITLE %></h5>
              <li><a href="/dashboard" class="breadcrumb-links"><%= translation.HUB_SPACES_TOOLBAR_DASHBOARD %></a></li>
              <li><a href="/spacelist" class="breadcrumb-links"><%= translation.HUB_SPACES_TOOLBAR_SPACES %></a></li>
              <li><%= translation.HUB_CREATE_SPACE_TOOLBAR %></li>
          </ol>
      </div>
      <section class="section-lg bg-white text-center main-section">
        <div class="shell">
          <h4><%= translation.HUB_CREATE_SPACE_TOOLBAR %></h4>
          <div class="range range-md-center">
            <div class="cell-md-10 cell-lg-8">
            <!-- RD Mailform-->
            <form class="rd-mailform rd-mailform_style-1 text-center" data-form-output="form-output-global" data-form-type="contact" method="post" action="bat/rd-mailform.php">
              <div class="form-wrap form-wrap_icon linear-icon-database-add">
                <input class="form-input" id="contact-sName" type="text" name="name" data-constraints="@Required">
                <label class="form-label" for="contact-sName"><%= translation.HUB_CREATE_SPACE_NAME %></label>
              </div>
              <div class="form-wrap form-wrap_icon linear-icon-users-plus">
              <input class="form-input" id="contact-sMember" type="email" name="email"  data-constraints="@Required">
              <label class="form-label" for="contact-sMember"><%= translation.HUB_CREATE_SPACE_TYPE_MEMBER_NAME %></label>
            </div>

            <div class="form-wrap form-wrap_icon linear-icon-key-hole">
              <input class="form-input" id="contact-sPasscode" type="text" name="passcode" data-constraints="@Required">
              <label class="form-label" for="contact-sPasscode"><%= translation.HUB_CREATE_SPACE_PASSCODE %> </label>
            </div>
            <div class="form-wrap form-wrap_icon linear-icon-home6">
              <input class="form-input" id="contact-sAddress" type="text" name="email" data-constraints="@Required">
              <label class="form-label" for="contact-sAddress"><%= translation.HUB_CREATE_SPACE_ADDRESS %></label>
              </div>
              <button id="btndiscard" class="button btn_discard " type="submit"><%= translation.HUB_CREATE_SPACE_DISCARD %> </button>
              &nbsp;&nbsp;
              &nbsp;&nbsp;
             <button id="btndone" class="button button-primary" type="submit"><%= translation.HUB_CREATE_SPACE_DONE %></button>
            </form>
          </div>
        </div>
       </div>
      </section>
    </div>

      <!-- include file for Script -->
      <% include _layouts/common-scripts %>
    <script>
        $(document).ready(function(){
            $("#spacesList").addClass("active");
        });
    </script>

    
<%include _layouts/footer %>  
  </body>
</html>
